function add() {
            var n1 =parseFloat(document.getElementById("num1").value);
       var n2 =parseFloat(document.getElementById("num2").value);
       if(isNaN(n1)  || isNaN(n2)){
       document.getElementById("result").innerText=" ENTER VADIL NUMBER ‚ùó";
       return;
     }
       document.getElementById("result").innerText= n1+ n2;
     
   }
    function subtract(){
      var n1 =parseFloat(document.getElementById("num1").value);
      var n2 =parseFloat(document.getElementById("num2").value);
      if(isNaN(n1)  || isNaN(n2)){
       document.getElementById("result").innerText=" ENTER VADIL NUMBER üö´";
       return;
     }
      document.getElementById("result").innerText= n1- n2;}
  function multiply(){
       var n1 =parseFloat(document.getElementById("num1").value);
       var n2 =parseFloat(document.getElementById("num2").value);  if(isNaN(n1)  || isNaN(n2)){
       document.getElementById("result").innerText=" ENTER VADIL NUMBER ‚ùó";
       return;
     }
       document.getElementById("result").innerText= n1 * n2;}
 function divide(){
        var n1 =parseFloat(document.getElementById("num1").value);
        var n2 =parseFloat(document.getElementById("num2").value);
        if(isNaN(n1)  || isNaN(n2)){
       document.getElementById("result").innerText=" ENTER VADIL NUMBER ‚ùó";
       return;
     }
        if (n2==0){
          document.getElementById("result").innerText ="canot divide by 0 ‚ùå"
        }
        else {
          document.getElementById("result").innerText=n1/n2; } }